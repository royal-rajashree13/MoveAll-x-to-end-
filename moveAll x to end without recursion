class sum{
    public static void main(String[] args) {
      String s= "axbxxxxcxxd";
      int index=s.length();
      int count=0;
      char current;
      String str="";
      for(int i=0; i<index;i++)
      {
        //copying the first character of the string to the variable current
        current=s.charAt(i);
        // checking whether the element is equal to 'x'
        if(current=='x')
        {
            count++;
        }
        else{
            str+=current;
        } 
    }
    //to add x to the End
    for(int j=0; j<count;j++)
            {
                str+='x';
            }
    //print the String with the x at end   
     System.out.println(str);
      

    }
}
